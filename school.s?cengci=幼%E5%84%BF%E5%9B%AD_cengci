<script type="text/javascript">
var _topLogout = ' <a style="margin-left:200px" href="javascript:;" onclick="javascript:popLogin(this);">[登录通行证]</a> <a href="http://reg.eol.cn/register.php" target="_blank">[注册]</a>   ';
var _topLogin = ' <font style="margin-left:200px" color="red">[*username*]</font>，欢迎登录! <a href="javascript:;" onclick="javascript:popLogin(this);">[打开]</a>        <a href="http://i.eol.cn/message_list.php" class="f_333" target="_blank">消息<span class="f_orange">([*msg*])</span></a> [<a id="_logoutlink" href=javascript:;" onclick="PassportPop.logout();">退出</a>]';
function _showTop(str)
{
	if($i('_ifrm')) document.body.appendChild($i('_ifrm'));
	var userName = decodeURIComponent(PassportPop.getCookie('eol_name'));
	if(userName.match(/^[a-zA-z0-9_]{13,}$/)) userName = userName.slice(0,12) + '...';
	else if(userName.match(/^[\u4e00-\u9fa5]{6,}$/)) userName = userName.slice(0,5) + '...';
	else if(userName.match(/[a-zA-z0-9_]+/) && userName.match(/[\u4e00-\u9fa5]+/) && userName.length>5) userName = userName.slice(0,5) + '...';

	var userMsg = PassportPop.getCookie('eol_msgnum') || 0;
	return str.replace(/\[\*username\*\]/g,userName).replace(/\[\*msg\*\]/, userMsg);
}
function _logout()
{
	try {
		var ifrm = document.createElement('<iframe name="_ifrm" src="http://peixun.eol.cn/logout.php"></iframe>');
	}catch(e) {
		var ifrm = document.createElement('iframe');
		ifrm.setAttribute('name','_ifrm');
		ifrm.src = 'http://peixun.eol.cn/logout.php';
	}
	ifrm.style.display = 'none';
	setTimeout(function(){document.body.appendChild(ifrm);}, 500);
	return '';
}
</script>
