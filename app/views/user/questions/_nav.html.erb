<%= tabs_tag(:namespace => :question_nav, :open_tabs => {:class => "nav nav-tabs" }, :builder => MenuTabBuilder) do |tab| %>
  <%= tab.index "我的问题", [@current_namespace, :questions].flatten %>
  <%= tab.answers "我的回答", [@current_namespace, :answers].flatten %>
  <%= link_to '我要提问', new_question_path, :class => 'btn btn-success pull-right span', target: '_blank' %>
<% end %>

