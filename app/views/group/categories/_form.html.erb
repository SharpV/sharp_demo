<%= form_for @category, :validate => true, url: group_group_categories_path(@group), remote: true, html: {class: 'form-inline', id: 'category-form', data: {type: "script", method: :post} } do |f| -%> 
  <%= f.text_field :name %>
  <%= f.submit '创建', :class=>'btn btn-primary', disable_with: "正在提交..." %>
<% end %>