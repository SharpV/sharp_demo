<div class='row'>
  <div class='span-l'>
    <h4 class='border'>
      [<%= @course.name %>] 文件上传
      <%= link_to [:webclass, @current_webclass, @course], class: "btn btn-success btn-small pull-right" do %>
        <i class="icon-ok icon-white"></i>
        <span>完成上传</span>
      <% end %>
    </h4>
    <%= render 'shared/file_upload_form', record: @medium, url: [:webclass, @current_webclass, @course, :media] %>
  </div>
  <div class='span-r pull-right'>
    <%= render 'webclass/courses/courses' %>
  </div>
</div>
