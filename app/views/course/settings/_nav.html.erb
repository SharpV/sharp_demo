<h3><%= @course.name %></h3>
<div class='row'>
  <div class='span2'>
    <%= tabs_tag(:namespace => :course_admin_nav, :open_tabs => {:class => "nav nav-tabs nav-stacked"}, :builder =>     MenuTabBuilder) do |tab| %>
      <%= tab.index "基本设置", course_course_settings_path(@course) %>
      <%= tab.categories "分类管理", admin_course_course_categories_path(@course) %>
      <%= tab.slots "课时管理", admin_course_course_sections_path(@course) %>
      <%= tab.members "学员管理", admin_course_course_members_path(@course) %>
    <% end %>
  </div>
</div>